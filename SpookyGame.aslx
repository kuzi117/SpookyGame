  <turnscript name="lantern_heart_turn_script">
    <enabled />
    <script><![CDATA[
      request (RunScript, "resetHeartCount")
      if (Got(lantern)) {
        if (lantern.fuel_level <= 0 and lantern.burnt_out <> True) {
          lantern.light_level = 0
          lantern.burnt_out = True
          msg ("Your lantern flame flickers for a moment and disappears. The room is engulfed in darkness.")
          lantern.fuel_level = lantern.fuel_level - lantern.light_level
        }
        else if (lantern.fuel_level - lantern.light_level < 10 and lantern.low_fuel <> True) {
          lantern.low_fuel = True
          if (lantern.light_level = 2) {
            lantern.light_level = 1
            msg ("Your lantern flame begins to gutter. You're forced to turn it down to maintain a steady flame. Your eyes adjust to the light level.")
          }
          else if (lantern.light_level = 1) {
            msg ("You notice that your lantern's reservoir is nearly fumes. Turning it up will probably be impossible until you get more fuel.")
          }
          lantern.fuel_level = lantern.fuel_level - lantern.light_level
        }
        else {
          lantern.fuel_level = lantern.fuel_level - lantern.light_level
        }
        if (game.debug) {
          msg ("FUEL DEBUG: " + lantern.fuel_level)
        }
      }
      if (game.heartRate < game.defaultHeartRate) {
        offset = game.defaultHeartRate - game.heartRate
        if (offset >= game.jumpScareAmount * 2 / 3) {
          msg ("Your heart is pounding and your breath is short.")
        }
        else if (offset >= game.jumpScareAmount * 1 / 3) {
          msg ("Your heart is beating quickly.")
        }
        else {
          msg ("Your heart is starting to calm down.")
        }
        newHeartRate = game.heartRate + 150
        if (newHeartRate <= game.defaultHeartRate) {
          SetHeartRate (newHeartRate)
        }
        else {
          SetHeartRate (game.defaultHeartRate)
        }
      }
      if (game.reenableCount < 1) {
        game.reenableCount = game.reenableCount + 1
      }
      else {
        EnableHeart
        game.helpShown = true
      }
      if (not game.heartEnabled and not game.gameover) {
        msg ("")
        msg ("(Time is paused. Enter any command to continue.)")
        if (not game.helpShown) {
          msg ("(Type HELP for gameplay help.)")
        }
      }
      if (game.saveTime) {
        msg ("<b>(Now is probably a good time to save!)</b>")
      }
    ]]></script>
