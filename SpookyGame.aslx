  <turnscript name="peter_turn_script">
    <script><![CDATA[
      Peter.distance = Peter.distance - 1
      if (game.debug) {
        msg ("DEBUG: PETER DISTANCE = " + Peter.distance)
      }
      if (nursery47.door_closed) {
        ResetPeter
      }
      else if (Peter.distance <= 0) {
        msg ("The child tackles the soul right out of you.")
        Die
      }
    ]]></script>
  </turnscript>
  <turnscript name="butcher_turn_script">
    <enabled />
    <script>
      if (kitchen.inside) {
        if (player.parent.bloodTrail and Got(lantern) and not lantern.light_level = 0) {
          kitchen.butcherTimer = 0
        }
        else {
          switch (kitchen.butcherTimer) {
            case (0) {
              msg ("The grunting has stopped.")
            }
            case (2) {
              msg ("You feel like you are being watched.")
            }
            case (3) {
              msg ("You hear heavy footsteps moving toward you.")
            }
            case (4) {
              msg ("The footsteps are getting closer.")
            }
            case (5) {
              msg ("You hear something rushing at you. The pungent odor of rotting meat fills your lungs, and you cough, closing your eyes. You feel a brush wind across your face, and hear movement in front of you. You open your eyes to see the glint of a butcher&#8217;s knife swing down.")
              Die
            }
          }
          kitchen.butcherTimer = kitchen.butcherTimer + 1
        }
      }
    </script>
  </turnscript>
  <function name="PrintRoomDesc" parameters="all">
    if (Got(lantern)) {
      if (lantern.light_level = 2) {
        switch (player.facing_num) {
          case (0) {
            msg (StringDictionaryItem (player.parent.descriptions, "brightnorth"))
          }
          case (1) {
            msg (StringDictionaryItem (player.parent.descriptions, "brighteast"))
          }
          case (2) {
            msg (StringDictionaryItem (player.parent.descriptions, "brightsouth"))
          }
          case (3) {
            msg (StringDictionaryItem (player.parent.descriptions, "brightwest"))
          }
        }
      }
      else if (lantern.light_level = 1) {
        switch (player.facing_num) {
          case (0) {
            msg (StringDictionaryItem (player.parent.descriptions, "dimnorth"))
          }
          case (1) {
            msg (StringDictionaryItem (player.parent.descriptions, "dimeast"))
          }
          case (2) {
            msg (StringDictionaryItem (player.parent.descriptions, "dimsouth"))
          }
          case (3) {
            msg (StringDictionaryItem (player.parent.descriptions, "dimwest"))
          }
        }
      }
      else {
        switch (player.facing_num) {
          case (0) {
            msg (StringDictionaryItem (player.parent.descriptions, "darknorth"))
          }
          case (1) {
            msg (StringDictionaryItem (player.parent.descriptions, "darkeast"))
          }
          case (2) {
            msg (StringDictionaryItem (player.parent.descriptions, "darksouth"))
          }
          case (3) {
            msg (StringDictionaryItem (player.parent.descriptions, "darkwest"))
          }
        }
      }
    }
    else {
      switch (player.facing_num) {
        case (0) {
          msg (StringDictionaryItem (player.parent.descriptions, "darknorth"))
        }
        case (1) {
          msg (StringDictionaryItem (player.parent.descriptions, "darkeast"))
        }
        case (2) {
          msg (StringDictionaryItem (player.parent.descriptions, "darksouth"))
        }
        case (3) {
          msg (StringDictionaryItem (player.parent.descriptions, "darkwest"))
        }
      }
    }
  </function>
  <function name="FaceNorth">
    player.facing_num = 0
    dictionary remove (player.statusattributes, "facing")
    dictionary add (player.statusattributes, "facing", StringListItem(player.facings, player.facing_num))
  </function>
  <function name="FaceEast">
    player.facing_num = 1
    dictionary remove (player.statusattributes, "facing")
    dictionary add (player.statusattributes, "facing", StringListItem(player.facings, player.facing_num))
  </function>
  <function name="FaceSouth">
    player.facing_num = 2
    dictionary remove (player.statusattributes, "facing")
    dictionary add (player.statusattributes, "facing", StringListItem(player.facings, player.facing_num))
  </function>
  <function name="FaceWest">
    player.facing_num = 3
    dictionary remove (player.statusattributes, "facing")
    dictionary add (player.statusattributes, "facing", StringListItem(player.facings, player.facing_num))
  </function>
  <function name="MoveAndFace" parameters="Dest, Facing">
    switch (Facing) {
      case (0) {
        FaceNorth
      }
      case (1) {
        FaceEast
      }
      case (2) {
        FaceSouth
      }
      case (3) {
        FaceWest
      }
    }
    MoveObject (player, Dest)
  </function>
  <function name="TimeOut" parameters="all">
    msg ("")
    msg ("Time passes.")
  </function>
  <function name="SetHeartRate" parameters="heartRate">
    game.heartRate = heartRate
    request (RunScript, "setHeartRate; " + game.heartRate)
  </function>
  <function name="ResetPeter">
    Peter.distance = Peter.start_distance
    Peter.chasing = False
    DisableTurnScript (peter_turn_script)
    msg ("DEBUG: PETER NO LONGER CHASING")
  </function>
  <function name="EnableHeart">
    request (RunScript, "enableHeart")
  </function>
  <function name="DisableHeart">
    request (RunScript, "disableHeart")
  </function>
  <function name="Die">
    DisableHeart
    msg ("You have died.")
    finish
  </function>
  <function name="GetJS" parameters="all">
    SetHeartRate (game.heartRate)
    if (game.heartEnabled) {
      EnableHeart
    }
    else {
      DisableHeart
    }
  </function>
  <function name="SetHeartCount" parameters="heartCount">
    game.heartCount = heartCount
    request (RunScript, "setHeartCount; " + game.heartCount)
  </function>
  <function name="UpdateHeartCount" parameters="heartCount">
    game.heartCount = heartCount
  </function>
