  <turnscript name="laila_turn_script">
    <enabled type="boolean">false</enabled>
    <script><![CDATA[
      if (not Laila.paused) {
        if (Laila.chase_state = 0 and GetRandomDouble() < .125) {
          msg ("You hear distant giggling...")
          Laila.chase_state = 1
          switch (player.facing_num) {
            case (0) {
              Laila.attack_from = 2
            }
            case (2) {
              Laila.attack_from = 0
            }
            case (1) {
              if (GetRandomDouble() < .5) {
                Laila.attack_from = 2
              }
              else {
                Laila.attack_from = 0
              }
            }
            case (3) {
              if (GetRandomDouble() < .5) {
                Laila.attack_from = 2
              }
              else {
                Laila.attack_from = 0
              }
            }
          }
          if (game.debug) {
            msg ("DEBUG: Laila advancing to stage 1: attacking from " + Laila.attack_from)
          }
        }
        else if (Laila.chase_state = 1) {
          // Don't advance if you're bright or looking at her
          if (lantern.light_level <> 2 or (lantern.light_level = 2 and (not (player.facing_num = Laila.attack_from)))) {
            // Tick at least once
            // On second 50/50 chance to advance or stay in 2
            // On third tick always move to state 3 (PS, these comments used to be in useful places, but quest moves them to a shitty place)
            if (Laila.sub_state = 0) {
              Laila.sub_state = 1
            }
            else if (Laila.sub_state = 1) {
              if (GetRandomDouble() < .5) {
                Laila.sub_state = 0
                Laila.chase_state = 2
                if (game.debug) {
                  msg ("DEBUG: Laila advancing to stage 2 on 2")
                }
              }
              else {
                Laila.sub_state = 2
              }
            }
            else if (Laila.sub_state = 2) {
              Laila.sub_state = 0
              Laila.chase_state = 2
              if (game.debug) {
                msg ("DEBUG: Laila advancing to stage 2 on 3")
              }
            }
          }
          else if (game.debug) {
            msg ("DEBUG: Laila waiting in stage 1")
          }
        }
        else if (Laila.chase_state = 2) {
          // Don't advance if you're bright or looking at her
          if ((lantern.light_level = 0) or ( (not (player.facing_num = Laila.attack_from)) and (lantern.light_level <> 0)) ) {
            // Tick at least once
            // On second 50/50 chance to advance or stay in 2
            // On third tick always move to state 3 (PS, these comments used to be in useful places, but quest moves them to a shitty place)
            if (Laila.sub_state = 0) {
              Laila.sub_state = 1
            }
            else if (Laila.sub_state = 1) {
              if (GetRandomDouble() < .5) {
                Laila.sub_state = 0
                Laila.chase_state = 3
                if (game.debug) {
                  msg ("DEBUG: Laila advancing to stage 3 on 2")
                }
              }
              else {
                Laila.sub_state = 2
              }
            }
            else if (Laila.sub_state = 2) {
              Laila.sub_state = 0
              Laila.chase_state = 3
              if (game.debug) {
                msg ("DEBUG: Laila advancing to stage 3 on 3")
              }
            }
          }
          else if (game.debug) {
            msg ("DEBUG: Laila waiting in stage 2")
          }
        }
        else if (Laila.chase_state = 3) {
          // Don't advance if you're bright or looking at her
          if ((lantern.light_level = 0) or (not (player.facing_num = Laila.attack_from))) {
            // Tick at least once
            // On second 50/50 chance to advance or stay in 3
            // On third tick always move to state 4 (PS, these comments used to be in useful places, but quest moves them to a shitty place)
            if (Laila.sub_state = 0) {
              Laila.sub_state = 1
            }
            else if (Laila.sub_state = 1) {
              if (GetRandomDouble() < .5) {
                Laila.sub_state = 0
                Laila.chase_state = 4
              }
              else {
                Laila.sub_state = 2
                if (game.debug) {
                  msg ("DEBUG: Laila advancing to stage 4 on 2")
                }
              }
            }
            else if (Laila.sub_state = 2) {
              Laila.sub_state = 0
              Laila.chase_state = 4
              if (game.debug) {
                msg ("DEBUG: Laila advancing to stage 4 on 3")
              }
            }
          }
          else if (game.debug) {
            msg ("DEBUG: Laila waiting in stage 3")
          }
        }
        else if (Laila.chase_state = 4) {
          if (lantern.light_level <> 0) {
            OutputTextNoBr ("You fumble and manage to turn up you lantern too late. ")
          }
          msg ("'Let’s play' she giggles, and tackles you to the ground. You wheeze, having the air knocked out of you. You feel like you were hit with a ton of bricks, and your limbs feel too heavy to move. You don’t know how a little girl could be so powerful. 'Why won’t you play with me' she screams, her face screwing up in malice. She begins to slam your head on the hard stone floor, disorienting you so there is no hope of fighting back. 'Why…' slam 'won’t' slam 'you' slam 'PLAY WITH ME!' she slams your head on the ground again, with more ferocity this time.")
          Die
        }
      }
      if (not Laila.paused) {
        // Now print descriptions
        if (player.facing_num = Laila.attack_from) {
          if (lantern.light_level = 2 and Laila.chase_state = 1) {
            msg ("You see a faint glimmering in the distance.")
          }
          else if (Laila.chase_state = 2) {
            if (lantern.light_level = 1) {
              msg ("You see a faint glimmering a few meters away from you.")
            }
            else if (lantern.light_level = 2) {
              msg ("You see a ghostly figure a few meters away from you.")
            }
          }
          else if (Laila.chase_state = 3) {
            if (lantern.light_level = 2) {
              msg ("You hear a shriek from the girl, 'I just wanted to play!', as she fades from existence.")
              // Reset Laila
              Laila.sub_state = 0
              Laila.chase_state = 0
              if (game.debug) {
                msg ("Debug: Laila reset.")
              }
            }
            else if (lantern.light_level = 1) {
              msg ("A ghostly figure resembling a girl floats a short distance from you. You can see excitement on her face.")
            }
            else {
              msg ("There's a faint glowing resembling a child floating a few feet off the ground not far in front off you.")
            }
          }
        }
      }
    ]]></script>
