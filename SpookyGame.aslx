  <turnscript name="geoffrey_turn_script">
    <enabled type="boolean">false</enabled>
    <script><![CDATA[
      if (player.parent <> player.lastPos) {
        if (game.heartCount = "4" or game.heartCount = "5" or game.heartCount = "6") {
          game.geoffreyCount = game.geoffreyCount - 1
          if (game.geoffreyCount < 0) {
            game.geoffreyCount = 0
          }
        }
        else {
          game.geoffreyCount = game.geoffreyCount + 1
        }
      }
      switch (game.geoffreyCount) {
        case (1) {
          msg ("You can hear a child grunting in the far end of the room as if he is agitated.")
        }
        case (2) {
          msg ("There is angry slamming coming from the far end of the room.")
        }
        case (3) {
          msg ("You hear a heavy sigh come from the small boy in the corner. He stands up, and looks irritated. He glares at you, and screams \"Shut up, shut up, shut up!\" He growls and starts charging at you, you take a step back and lose your balance, tripping on a stray toy. You tumble, face down, to the ground, and as you are trying to get back up you see his feet on the ground in front of you. You look up at him, and he is looking at you, coldly. \"I know how to make you shut up.\" He raises his fist, which is clenching a bone from the anatomy skeleton he was playing with before… though, it looks like a real human bone, fashioned into a shiv. \"At least I will have another toy...\" he swings his arm down, and stabs the bone into your neck.")
          Die
        }
      }
      player.lastPos = player.parent
    ]]></script>
  </turnscript>
  <function name="RunTurnScripts">
    if (IsGameRunning()) {
      if (game.fakeTurn) {
        if (game.menucallback = null) {
          foreach (turnscript, AllTurnScripts()) {
            if (GetBoolean(turnscript, "enabled")) {
              inscope = false
              if (turnscript.parent = game or turnscript.parent = null) {
                inscope = true
              }
              else {
                if (Contains(turnscript.parent, game.pov)) {
                  inscope = true
                }
              }
              if (inscope) {
                do (turnscript, "script")
              }
            }
          }
        }
        game.fakeTurn = false
      }
      else {
        game.fakeTurn = true
      }
    }
  </function>
